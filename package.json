{
  "name": "@nuxtjs/firebase",
  "version": "6.1.1",
  "description": "Intergrate Firebase into your Nuxt project.",
  "keywords": [
    "firebase",
    "firestore",
    "google",
    "googleauthentication",
    "nuxt",
    "realtimedatabase"
  ],
  "homepage": "https://firebase.nuxtjs.org/",
  "repository": "nuxt-community/firebase-module",
  "license": "MIT",
  "author": "Pascal Luther",
  "files": [
    "lib",
    "types/*.d.ts"
  ],
  "main": "lib/module.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .js,.vue lib test",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && jest",
    "commit": "npx git-cz",
    "docs": "cd docs && yarn dev"
  },
  "dependencies": {
    "consola": "^2.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.14.6",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/module-test-utils": "^1.6.3",
    "codecov": "^3.8.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.11.0",
    "firebase": "^7.23.0",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "standard-version": "^9.0.0"
  },
  "peerDependencies": {
    "firebase": "^7.23.0",
    "nuxt": "^2.14.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "optionalDependencies": {
    "firebase-admin": "^9.2.0"
  }
}
